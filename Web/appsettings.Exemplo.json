{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MYSQL": "server=<servidor>; port=<porta>; database=<database>; user=<login>; password=<senha>; charset=utf8mb4;",
    "SQLTE": "Data Source=<CaminhoDoArquivo.db>",
    "MSSQL": "Data Source=<servidor>;Initial Catalog=<database>;Persist Security Info=True;User Id=<login>;Password=<senha>;MultipleActiveResultSets=True;TrustServerCertificate=true"
  },
  "ConnectionStringsType": "MYSQL",
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://localhost:8093"
      }
    }
  }
}