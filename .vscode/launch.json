{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "BackEnd",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/Web/bin/Debug/net8.0/MaSistemas.Web.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Web",
      "stopAtEntry": false,
      "preLaunchTask": "build",
      "launchBrowser": {
        "enabled": false
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    {
      "name": "FrontEnd",
      "request": "launch",
      "cwd": "${workspaceFolder}/Web/VueApp",
      "type": "node",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "dev"
      ],
      "serverReadyAction": {
        "action": "startDebugging",
        "pattern": "Local:   (http?://\\S+|[0-9]+)",
        "name": "FrontEndDebug",
        "killOnServerStop": true,
      }
    },
    {
      "name": "FrontEndDebug",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8093/login?dominio=masuporte.com.br",
      "webRoot": "${workspaceFolder}/Web/VueApp/src",
      "sourceMaps": true,
      "disableNetworkCache": true,
      "trace": true,
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**"
      ]
    }
  ],
  "compounds": [
    {
      "name": "Launch",
      "configurations": [
        "BackEnd",
        "FrontEnd"
      ],
      "presentation": {
        "hidden": false,
        "group": "",
        "order": 1
      },
      "stopAll": true,
    }
  ]
}